@page
@using RazorPageSyntaxSamples.Models
@model RazorPageSyntaxSamples.Pages.RazorSyntax.IndexModel
@{
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cars[0].Id);
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cars[0].Brand);
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cars[0].Model);
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (Car currentCar in Model.Cars)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => currentCar.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => currentCar.Brand)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => currentCar.Model)
                </td>
                <td>
                </td>
            </tr>
        }
    </tbody>

</table>